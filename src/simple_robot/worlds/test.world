<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="test_dynamic_world">
    <include>
      <uri>model://sun</uri>
    </include>
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <model name="animated_box">
      <pose>-3 3 0.5 0 0 0</pose>
      <link name="link">
        <inertial>
          <mass>1.0</mass>
          <inertia>
            <ixx>0.083</ixx>
            <ixy>0.0</ixy>
            <ixz>0.0</ixz>
            <iyy>0.083</iyy>
            <iyz>0.0</iyz>
            <izz>0.083</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <box>
              <size>1 1 1</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>1 1 1</size>
            </box>
          </geometry>
          <material>
            <ambient>1 0 0 1</ambient>
            <diffuse>1 0 0 1</diffuse>
            <specular>0.1 0.1 0.1 1</specular>
          </material>
        </visual>
      </link>
      <plugin name="box_mover" filename="libgazebo_ros_planar_move.so">
        <ros>
          <namespace>/box</namespace>
          <remapping>cmd_vel:=/box_cmd_vel</remapping>
          <remapping>odom:=/box_odom</remapping>
        </ros>
        <update_rate>20</update_rate>
        <publish_odom>true</publish_odom>
        <publish_odom_tf>true</publish_odom_tf>
        <odometry_frame>odom</odometry_frame>
        <robot_base_frame>link</robot_base_frame>
      </plugin>
    </model>

    <model name="animated_cylinder">
        <pose>3 -3 0.5 0 0 0</pose>
        <link name="link">
            <inertial>
                <mass>1.0</mass>
                <inertia>
                    <ixx>0.083</ixx>
                    <ixy>0.0</ixy>
                    <ixz>0.0</ixz>
                    <iyy>0.083</iyy>
                    <iyz>0.0</iyz>
                    <izz>0.083</izz>
                </inertia>
            </inertial>
            <collision name="collision">
                <geometry>
                    <cylinder>
                        <radius>0.5</radius>
                        <length>1.0</length>
                    </cylinder>
                </geometry>
            </collision>
            <visual name="visual">
                <geometry>
                    <cylinder>
                        <radius>0.5</radius>
                        <length>1.0</length>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>0 0 1 1</ambient>
                    <diffuse>0 0 1 1</diffuse>
                    <specular>0.1 0.1 0.1 1</specular>
                </material>
            </visual>
        </link>
        <plugin name="cylinder_mover" filename="libgazebo_ros_planar_move.so">
            <ros>
              <namespace>/cylinder</namespace>
              <remapping>cmd_vel:=/cylinder_cmd_vel</remapping>
              <remapping>odom:=/cylinder_odom</remapping>
            </ros>
            <update_rate>20</update_rate>
            <publish_odom>true</publish_odom>
            <publish_odom_tf>true</publish_odom_tf>
            <odometry_frame>odom</odometry_frame>
            <robot_base_frame>link</robot_base_frame>
        </plugin>
    </model>

  </world>
</sdf>

